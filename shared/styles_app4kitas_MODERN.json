    "dark": {
      "primary": "#43B97F",
      "primaryDark": "#37996B",
      "accent": "#FFE066",
      "accentHover": "#FFD54F",
      "background": "#181C24",
      "surface": "#232B36",
      "surfaceAlt": "#222B36",
      "surfaceHover": "#2A3440",
      "error": "#FF5A5F",
      "textPrimary": "#E0E0E0",
      "textSecondary": "#BDBDBD",
      "text": "#E0E0E0",
